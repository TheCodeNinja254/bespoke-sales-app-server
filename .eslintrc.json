{
  "extends": "airbnb-base",
  "rules": {
    "max-len": [1, 140, 2, {
      "ignoreComments": true}],
    "no-param-reassign": 0,
    "quote-props": [1, "consistent-as-needed"],
    "no-cond-assign": [2, "except-parens"],
    "radix": 0,
    "space-infix-ops": 0,
    "linebreak-style": 0,
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "default-case": 0,
    "no-else-return": 0,
    "class-methods-use-this": 0,
    "no-multiple-empty-lines": "error",
    "quotes": 0
  }
}
